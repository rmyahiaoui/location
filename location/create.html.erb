<h1>Location#create</h1>
<p>Find me in app/views/location/create.html.erb</p>

<%= form_for :location, url: location_path do |f| %>
  <p>
    <%= f.label :Adress %><br>
    <%= f.text_area :adress %>
  </p>

  <p>
    <%= f.label :Ville %><br>
    <%= f.text_area :ville %>
  </p>
 
  <p>
    <%= f.label :Code_postal %><br>
    <%= f.text_area :code_postal %>
  </p>

  <p>
    <%= f.label :Pay %><br>
    <%= f.text_area :pay %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
